        <activity android:name=".TambahIkan"></activity>
        <activity
            android:name=".dashboardn"
            android:label="@string/title_activity_dashboardn" />
        <activity android:name=".registerk" />
        <activity android:name=".landing" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>